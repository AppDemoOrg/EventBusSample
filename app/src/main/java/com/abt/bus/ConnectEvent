
package com.abt.bus;

public class ConnectEvent {
    private IMqttToken token;
    private boolean status;

    public ConnectEvent() {};

    public boolean getStatus() {
        return status;
    }

    public void setStatus(boolean status) {
        this.status = status;
    }

    public IMqttToken getToken() {
        return token;
    }

    public void setToken(IMqttToken token) {
        this.token = token;
    }
}
